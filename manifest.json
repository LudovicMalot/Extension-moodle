{
	"manifest_version": 3,
	"name": "Moodle quizz",
	"version": "1.0",
	"permissions": ["storage", "activeTab"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "./popup/popup.html",
		"default_icon": "logo.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["scripts/content.js"]
		}
	]
}
